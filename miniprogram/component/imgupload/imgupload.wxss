.imgBox{
  background: #fff;
}
.imglist{
  width:100%;
  display: flex;
  flex-wrap:wrap;
  justify-content: flex-start;
  align-items: center
}
.imgsize{
  width:180rpx;
  height:180rpx;
  border:1rpx solid #ccc
}
.upicon{
  width:70rpx;
  height:70rpx;
  margin-left:30rpx;
  margin-bottom:30rpx;
  background-image:url(data:image/png;base64,);
  background-size:cover ;
}
.imgItem{
  width:180rpx;
  height:180rpx;
  margin: 20rpx;
  position: relative;
  border: 1rpx solid #999;
}
.imgclose{
  width:50rpx;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
  position: absolute;
  top:0;
  right:0;
  z-index: 3;
  background: #666;
  color:#fff;
  font-size: 32rpx;
}