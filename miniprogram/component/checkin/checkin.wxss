.checklist{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.checkicon{
  width:50rpx;
  height:50rpx;
}
.checkicon1{
  /* 蓝色 */
  background-image:url("data:image/png;base64,");
  background-size:cover ;
}
.checkicon2{
  /* 橙色 */
    background-image:url("data:image/png;base64,");
  background-size:cover ;
}
.checktext{
  font-size: 28rpx;
}
.checktext1{
  color:#7691fc
}
.checktext2{
  color:#f98723
}
.eventBg{
  position: absolute;
  top:0;
  right:0;
  bottom:0;
  left:0;
  z-index: 99992;
  background:rgba(0, 0, 0, 0.5);
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center
}
.nosubList{
  width:550rpx;
  max-height:680rpx;
  background: #fff;
  position: relative
}
.notitle{
  height:100rpx;
  line-height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content:center;
  align-items: center;
  border-bottom:10rpx solid #eee;
}
.notitle1{
  font-weight: bold;
  color:#666
}
.closeBtn{
  width:80rpx;
  height:80rpx;
  position: absolute;
  background: #fff; 
  top:-79rpx;
  right:0;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center
}
.closeSize{
  width:50rpx;
  height:50rpx;
  opacity: 0.6
}
.closedbg{
  background-image:url("data:image/png;base64,");
  background-size:cover ;
}
.btnbox{
  width:550rpx;
  height:100rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center
}
.btnCopy{
  width:320rpx;
  height:80rpx;
  background: #7691fc;
  line-height: 80rpx;
  text-align: center;
  color:#fff;
}
.nocon{
  padding:10rpx 50rpx;
}