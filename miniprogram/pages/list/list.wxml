<view wx:for="{{audioList}}" wx:key='audioList'>
  <view 
  bindtap="gotoaudio" 
  data-title="{{item.title}}" 
  data-fid = "{{item.uripath}}" 
  data-type = "{{item.type}}"
  data-tips = "{{item.tips}}"
  style="padding:20rpx; background:#eee; margin-bottom:20rpx;"
  >
    <view>{{item.title}}</view>
    <view>{{item.uripath}}</view>
  </view>

</view>