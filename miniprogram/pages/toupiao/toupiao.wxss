.tableborder{
  margin-bottom: 20rpx;
  border-bottom: 12rpx solid #eee;
}
.jlBottom{
  position: fixed;
  left:0;
  bottom:0;
  z-index: 99;
  width:100%;
  height:96rpx;
}
.tablearea{
  margin-bottom:100rpx;
}
.addQuestion{
  width:100%;
  height:130rpx;
  background: #fff;
  margin-top:15rpx;
  border-top:15rpx solid #f4f4f4;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center
}
.addQuestion .icon{
  color:#7793f4;
  margin-left:100rpx;
  margin-right:10rpx;
  font-size: 50rpx;
  font-weight: bold;
}
.addQuestion .val{
  color:#7793f4;
}
.paramslist{
  border-top:20rpx solid #f4f4f4;
  padding:20rpx;
  position: relative;
}
.inputView{
  color:#bbb;
  border:1rpx solid #ccc;
  padding:10rpx;
}
.optionItem{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  height:60rpx;
  line-height: 60rpx;
}
.radioIcon{
  width:30rpx;
  height:30rpx;
  border-radius:20rpx;
  border:1rpx solid #999;
}
.checkboxIcon{
  width: 30rpx;
  height:30rpx;
  border:1rpx solid #999;
}
.warpItem{
  position: relative
}
.subMenu{
  position: absolute;
  width:100rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  top:-12rpx;
  right:-12rpx;
  z-index: 99;
  font-size: 36rpx;
  font-weight: bold
}
/* pages/tableadd/tableadd.wxss */
page{
  background: #fff;
}
.typeItem{
  height:40rpx;
  line-height:40rpx;
  padding:25rpx;
}
.inputItem{
  height:100rpx;
  line-height: 100rpx;
  border-bottom:1rpx solid #dedede;
}
.isMustFill{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding:50rpx;
}
.selectBox{
  padding:10rpx 30rpx;
}
.seleceoption{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  height:80rpx;
  line-height: 80rpx;
  border-bottom:1rpx solid #999;
}
.addoption{
  color:#7793f4;
  height:90rpx;
  line-height:90rpx;
}

.styleItem{
  width:100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center
}