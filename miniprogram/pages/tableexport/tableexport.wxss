.excelPath{
  border:1rpx solid #ccc;
  padding:20rpx;
  background: #ececec;
  word-wrap: break-word;
  margin:25rpx 0;
}
.eventBox{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center
}
.eventBtn{
  width:100%;
  height:90rpx;
  line-height: 90rpx;
  text-align: center;
  display: block;
  border:1rpx solid #ccc;
  margin:20rpx 0;
}
.copy{
  background: #2ad82e;
  color:#fff;
}
.open{
  background: #2069f8;
  color:#fff;
}
.Course{
  margin-top:20rpx;
}
.keypoint{
  font-weight: bold
}
.Course view{
  margin-bottom:10rpx;
}
.progressBox{
  position: fixed;
  top:0;
  right:0;
  bottom:0;
  left:0;
  background: rgba(0,0,0,0.3);
  z-index: 999;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.progress{
  width:500rpx;
  height:20rpx;
  border-radius:10rpx;
  border:1rpx solid #999;
  
  box-sizing: border-box;
  background: #ddd;
}
.jindu{
  height:20rpx;
  background: #3f8bfc;
  border-radius: 10rpx;
}
.loadfont{
  width: 500rpx;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
  color:#fff;
  font-size:28rpx;
}