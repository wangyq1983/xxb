page{
  background: #fff;
}
.zlTitle{
  padding:20rpx;
  margin-bottom: 20rpx;
}
.progressBox{
  position: fixed;
  top:0;
  right:0;
  bottom:0;
  left:0;
  background: rgba(0,0,0,0.3);
  z-index: 999;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.progress{
  width:500rpx;
  height:20rpx;
  border-radius:10rpx;
  border:1rpx solid #999;
  
  box-sizing: border-box;
  background: #ddd;
}
.jindu{
  height:20rpx;
  background: #3f8bfc;
  border-radius: 10rpx;
}
.loadfont{
  width: 500rpx;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
  color:#fff;
  font-size:28rpx;
}
.ziliaoItem{
  padding:30rpx 20rpx; 
  background:#f1faff; 
  margin-bottom:20rpx;
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}