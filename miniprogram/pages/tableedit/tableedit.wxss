.tableborder{
  margin-bottom: 20rpx;
}
.jlBottom{
  position: fixed;
  left:0;
  bottom:0;
  z-index: 99;
  width:100%;
  height:96rpx;
}
.tablearea{
  margin-bottom:100rpx;
}
.addQuestion{
  width:100%;
  height:130rpx;
  background: #fff;
  margin-top:15rpx;
  border-top:15rpx solid #f4f4f4;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center
}
.addQuestion .icon{
  color:#7793f4;
  margin-left:100rpx;
  margin-right:10rpx;
  font-size: 50rpx;
  font-weight: bold;
}
.addQuestion .val{
  color:#7793f4;
}
.paramslist{
  border-top:20rpx solid #f4f4f4;
  padding:20rpx;
  position: relative;
}
.inputView{
  color:#bbb;
  border:1rpx solid #ccc;
  padding:10rpx;
  font-size: 28rpx;
}
.textareaView{
  color:#bbb;
  border:1rpx solid #ccc;
  height:100rpx;
  padding:10rpx;
  font-size: 28rpx;
}
.optionItem{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  height:60rpx;
  line-height: 60rpx;
}
.radioIcon{
  width:30rpx;
  height:30rpx;
  border-radius:20rpx;
  border:1rpx solid #999;
}
.checkboxIcon{
  width: 30rpx;
  height:30rpx;
  border:1rpx solid #999;
}
.warpItem{
  position: relative
}
.subMenu{
  position: absolute;
  width:100rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  top:-12rpx;
  right:-12rpx;
  z-index: 99;
  font-size: 36rpx;
  font-weight: bold
}
.titleFont{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.titleFont .idx{
  color:#2069f8;
  font-weight: bold;
  font-size: 30rpx;
  padding-right:20rpx;
}
.titleFont .value{
  color:#333;
  font-size: 30rpx;
}
.description{
  font-size: 22rpx;
  color:#999;
  padding: 6rpx 0;
}


.tableFillBox{
  
}
.tableFillBox input{
  border:1rpx solid #ccc;
  height:60rpx;
}
.tableFillBox input:focus{
  border:1rpx solid #7691fc
}
.tableFillBox textarea{
  width:100%;
  border:1rpx solid #ccc;
  height:128rpx;
}
.nameItem{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.titleFont{
  font-size: 30rpx;
  color:#333;
}
.labelFont{
  font-size: 30rpx;
  color:#666;
}
.description{
  font-size: 22rpx;
  color:#999;
  padding: 6rpx 0;
}
.mustfillStyle{
  font-size: 22rpx;
  color:#f00;
  padding-left:10rpx
}